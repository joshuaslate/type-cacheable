{
  "name": "@type-cacheable/core",
  "version": "12.0.0",
  "description": "TypeScript-based caching decorators",
  "main": "dist/index.js",
  "module": "./index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
