  },
  "homepage": "https://github.com/joshuaslate/type-cacheable#readme",
  "devDependencies": {
    "@types/node": "^20.1.0",
    "@types/serialize-javascript": "^5.0.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
