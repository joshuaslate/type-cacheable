{
  "name": "@type-cacheable/lru-cache-adapter",
  "version": "18.0.2",
  "description": "Adapter for using lru-cache with type-cacheable",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
