{
  "name": "type-cacheable",
  "version": "1.0.0",
  "description": "TypeScript-based caching decorators",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "node"
    ]
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "test": "jest --coverage --runInBand",
    "test:watch": "jest --watch --runInBand",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuaslate/type-cacheable.git"
  },
  "keywords": [
    "cache",
    "typescript",
    "redis"
  ],
  "author": "Joshua Anderson Slate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshuaslate/type-cacheable/issues"
  },
  "homepage": "https://github.com/joshuaslate/type-cacheable#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/md5": "^2.1.33",
    "@types/node": "^12.7.4",
    "@types/serialize-javascript": "^1.5.0",
    "jest": "^24.9.0",
    "node-cache": "^4.2.1",
    "redis": "^2.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@types/node-cache": "^4.1.3",
    "@types/redis": "^2.8.13",
    "md5": "^2.2.1",
    "serialize-javascript": "^2.1.0"
  }
}
